 body 
     width: 100%;
     height: 100%;
     font: 14px/1.5 'Microsoft Yahei'
     background-color: #f1f1f1;  
     color: #666;
h1,h2 
    text-align: center;
    color: #666;
h1
    font-size: 30px;
h2 
    font-size: 26px;    
.clearfix
    *zooom: 1
.clearfix:after
    content: '.';
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
    
// mixin
box-sizing(n)
  -webkit-box-sizing n
  -moz-box-sizing n
  -ms-box-sizing n
  -o-box-sizing n
  box-sizing n
// column stylesheet

*[class^= col-]
    box-sizing: border-box;
    float: left;
    height: 50px;
    margin: 10px 0;
    border-left: 10px solid #fff;
    border-right: 10px solid #fff;
    background-color: rgba(86, 61, 124, 0.15);
    text-align: center;
    line-height: 50px;
    
.s-inner,.l-inner
    display: none;

.container, .container-full
    padding: 10px;
    background-color: #fff;
@media screen and (max-width:md)
  .s-inner
      display: block
@media screen and (min-width:md)
  .l-inner
      display: block
